@model List<string>
@{
    ViewBag.Title = "Stub";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="myTitleComic">Winners</h2>

@if (Model != null)
{
    for (int i = 0; i < Model.Count() / 4; i++)
    {
        <div class="stub">
            @Html.Label(@Model[i * 4])
            @Html.ActionLink(Model[i * 4 + 1], "Team", "Home", routeValues: new { TeamId = Model[i * 4 + 2], fedSeason = Model[i * 4 + 3] }, htmlAttributes: null)                
        </div>
    }
}
else
{
    <div class="myTitleComic">
        <h2>Request failed</h2>
    </div>
}